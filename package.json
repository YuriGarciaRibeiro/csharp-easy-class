{
  "name": "csharp-easy-class",
  "displayName": "C# Easy Class",
  "description": "Paste a class from clipboard",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "menus": {
      "explorer/context": [
        {
          "command": "csharp-easy-class.pasteClass",
          "group": "navigation"
        }
      ]
    },
    "commands": [
      {
        "command": "csharp-easy-class.pasteClass",
        "title": "Paste Class from Clipboard"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/YuriGarciaRibeiro/csharp-easy-class.git"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.97.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "eslint": "^9.19.0",
    "typescript": "^5.7.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}